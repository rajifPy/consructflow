{
  "name": "constructflow-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "build:crew": "cd apps/crew-app && npm run build",
    "build:procure": "cd apps/procure-app && npm run build",
    "build:equip": "cd apps/equip-app && npm run build"
  },
  "devDependencies": {
    "turbo": "^1.11.0"
  },
  "packageManager": "npm@10.2.4"
}
