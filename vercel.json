{
  "version": 2,
  "builds": [
    {
      "src": "apps/procure-app/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/equip-app/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/crew-app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/procure/(.*)",
      "dest": "apps/procure-app/$1"
    },
    {
      "src": "/equip/(.*)",
      "dest": "apps/equip-app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "apps/crew-app/$1"
    }
  ]
}
